<?xml version='1.0' encoding='utf-8'?>
<widget id="com.hsrm.pushnotification" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>HSRM Push</name>
    <description>
        Eine App zur Forschung
    </description>
    <author email="m142679@googlemail.com" href="http://phonegap.com">
        Maximilian Wick
    </author>
    <content src="index.html" />
    <gap:plugin name="pushbots-cordova-plugin" source="npm" spec="1.5.x" />
    <icon src="www/img/logo.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="15" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-7.1.0" />
</widget>
